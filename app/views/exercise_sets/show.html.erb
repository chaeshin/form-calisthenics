<div>
  <h1 class="custom-margin">
    <%#= @workout_session.workout.name %>
    <%= @exercise.name %>
  </h1>

  <div>
    <%# sets and reps here: %>
    <h6 class="exercise-info"><%= @exercise.sets %> x
      <% if @exercise.lower_reps.nil? && @exercise.upper_reps.nil?  %>
        <%= @exercise.hold_time %>
      <% else %>
        <%= @exercise.lower_reps %>-<%= @exercise.upper_reps %>
      <% end %>
    </h6>
    <%# rest time:here %>
    <h6 class="exercise-info">| rest: <%= @exercise.rest %>s</h6>
  </div>

  <div><%= @reps %></div>

  <%= cl_video_tag @exercise_set.video.key, width: 410, resource_type: 'video', controls: true %>
</div>
