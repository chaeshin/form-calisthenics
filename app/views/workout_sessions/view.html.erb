<div class="container">
  <h1 class="custom-margin mb-1">
    <%= @workout_session.workout.name %>
  </h1>
  <span>
    <h3 class="m-0"><%= @workout_session.start_time.strftime("%b %d, %Y") %></h3>
  </span>
  <% @exercises.each_with_index do |exercise, index| %>
    <% if exercise.exercise_sets.where(workout_session: @workout_session).count.positive?%>
      <%= render 'shared/exercise_done', exercise: exercise, index_exercise: index, workout_session: @workout_session %>
    <% end %>
  <% end %>
</div>
